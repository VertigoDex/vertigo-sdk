{
  "name": "@vertigo-amm/vertigo-sdk",
  "version": "2.0.0",
  "description": "Official TypeScript SDK for the Vertigo AMM protocol on Solana",
  "homepage": "https://vertigo.so",
  "repository": {
    "type": "git",
    "url": "https://github.com/vertigo-protocol/vertigo-sdk"
  },
  "bugs": {
    "url": "https://github.com/vertigo-protocol/vertigo-sdk/issues"
  },
  "bin": {
    "vertigo": "./dist/cli/index.js"
  },
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    },
    "./api": {
      "types": "./dist/src/api/index.d.ts",
      "import": "./dist/src/api/index.mjs",
      "require": "./dist/src/api/index.js"
    },
    "./client": {
      "types": "./dist/src/client/index.d.ts",
      "import": "./dist/src/client/index.mjs",
      "require": "./dist/src/client/index.js"
    },
    "./utils": {
      "types": "./dist/src/utils/index.d.ts",
      "import": "./dist/src/utils/index.mjs",
      "require": "./dist/src/utils/index.js"
    },
    "./constants": {
      "types": "./dist/src/core/constants.d.ts",
      "import": "./dist/src/core/constants.mjs",
      "require": "./dist/src/core/constants.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "solana",
    "anchor",
    "vertigo",
    "amm",
    "defi",
    "sdk",
    "typescript",
    "swap",
    "liquidity",
    "pool"
  ],
  "author": "Vertigo Protocol",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc --noEmit false --skipLibCheck --project tsconfig.build.json && yarn build:esm && yarn copy-files",
    "build:esm": "tsc --module esnext --outDir dist/esm --noEmit false --skipLibCheck --project tsconfig.build.json && find dist/esm -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.mjs\"' _ {} \\;",
    "copy-files": "mkdir -p dist/target && cp -r target/* dist/target/",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration/*.integration.test.ts",
    "test:devnet": "tsx tests/integration/test-runner.ts test",
    "test:devnet:verbose": "tsx tests/integration/test-runner.ts test --verbose",
    "test:devnet:export": "tsx tests/integration/test-runner.ts test --export test-results.json",
    "test:devnet:category": "tsx tests/integration/test-runner.ts test --category",
    "test:devnet:help": "tsx tests/integration/test-runner.ts --help",
    "test:full": "tsx tests/integration/full-integration.test.ts",
    "lint": "prettier --check \"src/**/*.{js,ts}\"",
    "lint:fix": "prettier --write \"src/**/*.{js,ts}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build && yarn test",
    "cli:dev": "tsx src/cli/index.ts",
    "examples": "tsx",
    "docs": "typedoc --out docs src/index.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@coral-xyz/anchor-cli": "^0.31.2",
    "@solana/spl-token": "^0.4.12",
    "@solana/wallet-adapter-base": "^0.9.26",
    "bs58": "^6.0.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "inquirer": "^12.5.2",
    "tsx": "^4.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/node": "^22.14.1",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "prettier": "^2.6.2",
    "typescript": "^5.7.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "yargs": "^17.7.2"
  }
}
